{
  "name": "frontend_nuxt",
  "version": "0.1.0",
  "description": "Frontend Nuxt 4 + TailwindCSS 4 skeleton",
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt preview",
    "typecheck": "npx nuxi prepare && npx vue-tsc --noEmit -p tsconfig.json",
    "journey:scaffold": "node scripts/journey/scaffold.mjs",
    "guards:ci": "npm run no-remote-assets:guard && npm run no-v-html:guard && npm run no-innerhtml:guard && npm run no-console:guard && npm run data-only:guard && npm run bilan:guard && npm run bilan-vm:guard && npm run bilan-vm-keys:guard && npm run journey:guard && npm run journey-entrypoint:guard && npm run journey-unknown-slug:guard && npm run journey-manifest-registry:guard && npm run journey-step-allowlist:guard && npm run journey-scaffold:guard && npm run journey-scaffold-cli-syntax:guard && npm run stepshell-nav:guard && npm run journey-e0-shell:guard && npm run engagement:guard && npm run markdown-safe:guard && npm run reco:guard && npm run resourcesdeeplink:guard && npm run resourceslib:r2:guard && npm run ds:guard && npm run stepshell:r1:guard && npm run stepshell:r2:guard && npm run states:r1:guard && npm run states:r2:guard",
    "bilan:guard": "npx nuxi prepare && tsx --tsconfig .nuxt/tsconfig.json tools/bilan_adapter_guard.ts",
    "bilan-vm:guard": "tsx tools/bilan_vm_contract_guard.ts",
    "bilan-vm-keys:guard": "node scripts/guards/bilan-vm-forbidden-keys-r1.mjs",
    "journey:guard": "tsx tools/journey_manifest_guard.ts",
    "journey-entrypoint:guard": "node scripts/guards/journey-entrypoint-e0.mjs",
    "journey-unknown-slug:guard": "node scripts/guards/journey-unknown-slug-404-r1.mjs",
    "journey-manifest-registry:guard": "node scripts/guards/journey-manifest-registry-completeness-r1.mjs",
    "journey-step-allowlist:guard": "node scripts/guards/journey-step-allowlist-r1.mjs",
    "journey-scaffold:guard": "node scripts/guards/journey-scaffold-r1.mjs",
    "journey-scaffold-cli-syntax:guard": "node scripts/guards/journey-scaffold-cli-syntax-r1.mjs",
    "journey-e0-shell:guard": "node scripts/guards/journey-e0-shell-r1.mjs",
    "stepshell-nav:guard": "node scripts/guards/step-shell-nav-r1.mjs",
    "journey:nodup": "tsx tools/journey_no_wrappers_guard.ts",
    "no-console:guard": "tsx tools/no_console_guard.ts",
    "engagement:guard": "tsx tools/engagement_guard.ts",
    "no-v-html:guard": "tsx tools/no_v_html_guard.ts",
    "no-innerhtml:guard": "node scripts/guards/no-innerhtml.mjs",
    "data-only:guard": "tsx tools/data_only_strings_guard.ts",
    "markdown-safe:guard": "tsx tools/markdown_safe_guard.ts",
    "reco:guard": "tsx tools/recommendations_guard.ts",
    "cta:guard": "node scripts/guards/no-legacy-cta-classes.mjs",
    "card:guard": "node scripts/guards/no-ad-hoc-card-wrappers.mjs",
    "ppcard:guard": "node scripts/guards/no-ppcard-style-classes.mjs",
    "badgeA:guard": "node scripts/guards/no-legacy-badge-classes-a.mjs",
    "badgeB:guard": "node scripts/guards/no-legacy-badge-classes-b.mjs",
    "chip:guard": "node scripts/guards/no-legacy-chip-classes-c.mjs",
    "sectionheader:guard": "node scripts/guards/no-legacy-sectionheader-classes.mjs",
    "pageheaderC1:guard": "node scripts/guards/no-legacy-pageheader-classes-c1.mjs",
    "pageheaderC3:guard": "node scripts/guards/no-legacy-pageheader-classes-c3.mjs",
    "pageheaderC2:guard": "node scripts/guards/no-legacy-pageheader-classes-c2.mjs",
    "scale:guard": "node scripts/guards/no-legacy-scale-classes-q1.mjs",
    "progress:guard": "node scripts/guards/no-legacy-progress-classes-q3.mjs",
    "questionnav:guard": "node scripts/guards/no-legacy-question-nav-q4.mjs",
    "skip:guard": "node scripts/guards/no-legacy-skip-patterns-q4.mjs",
    "questionwrapper:guard": "node scripts/guards/no-legacy-question-wrapper-q2.mjs",
    "questionnaireshell:guard": "node scripts/guards/no-legacy-questionnaire-shell-q5.mjs",
    "no-remote-assets:guard": "node scripts/guards/no-remote-assets.mjs",
    "bilanshell:guard": "node scripts/guards/no-legacy-bilan-shell-r1.mjs",
    "bilancard:guard": "node scripts/guards/no-legacy-bilan-cards-r2.mjs",
    "badgechip:r3a:guard": "node scripts/guards/no-legacy-badge-chip-r3a.mjs",
    "bilansectionheader:guard": "node scripts/guards/no-legacy-bilan-sectionheaders-r4.mjs",
    "hypatter:r1:guard": "node scripts/guards/no-legacy-hyp-atter-r1.mjs",
    "resources:r1:guard": "node scripts/guards/no-legacy-resources-r1.mjs",
    "resourceslib:r1:guard": "node scripts/guards/no-legacy-resources-library-r1.mjs",
    "resourceslib:r2:guard": "node scripts/guards/no-menhir-resources-page-r2.mjs",
    "resourcesdeeplink:guard": "RESOURCESDEEPLINK_SCOPE=r3 node scripts/guards/no-manual-resources-query.mjs",
    "resourcesdeeplink:r1:guard": "RESOURCESDEEPLINK_SCOPE=pilot node scripts/guards/no-manual-resources-query.mjs",
    "resourcesdeeplink:r2:guard": "RESOURCESDEEPLINK_SCOPE=r2 node scripts/guards/no-manual-resources-query.mjs",
    "resourcesdeeplink:r3:guard": "RESOURCESDEEPLINK_SCOPE=r3 node scripts/guards/no-manual-resources-query.mjs",
    "ds:guard": "node scripts/guards/no-ds-page-in-prod.mjs",
    "stepshell:r1:guard": "node scripts/guards/no-legacy-step-shell-r1.mjs",
    "stepshell:r2:guard": "node scripts/guards/no-legacy-step-shell-r2.mjs",
    "states:r1:guard": "node scripts/guards/no-ad-hoc-states-r1.mjs",
    "states:r2:guard": "node scripts/guards/no-ad-hoc-states-r2.mjs",
    "ssot:lint": "cd .. && python3 tools/ssot_lint.py",
    "ssot:lint:strict": "cd .. && python3 tools/ssot_lint.py --strict",
    "api:generate": "npx openapi-typescript@7.4.4 ../docs/30-tech_specs/backend/openapi.v1.yaml --output ./app/types/api.generated.ts --export-type --alphabetize",
    "api:validate": "cd .. && python3 tools/openapi_validate.py",
    "api:mock": "npx @stoplight/prism-cli@5.11.2 mock ../docs/30-tech_specs/backend/openapi.v1.yaml --port 4010"
  },
  "keywords": [
    "nuxt",
    "tailwindcss",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "nuxt": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^24.10.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.5"
  },
  "engines": {
    "node": ">=20.19.0 <21 || >=22.12.0"
  }
}
